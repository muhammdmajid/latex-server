{
  "name": "latex-server",
  "version": "1.0.0",
  "description": "An Express.js template with TypeScript, ESLint and Prettier",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec tsx src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "npx eslint \"src/**/*.{js,ts,jsx,tsx}\"",
    "lint:fix": "npx eslint --fix \"src/**/*.{js,ts,jsx,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,ts,css,html,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,css,html,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muhammdmajid/latex-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/muhammdmajid/latex-server/issues"
  },
  "homepage": "https://github.com/muhammdmajid/latex-server#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "adm-zip": "^0.5.16",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.5.0",
    "envalid": "^8.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "fs-extra": "^11.3.0",
    "http-status-codes": "^2.3.0",
    "multer": "^1.4.5-lts.2",
    "node-unrar-js": "^2.0.2",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "tar": "^7.4.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/adm-zip": "^0.5.7",
    "@types/cookie-parser": "^1.4.8",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "eslint": "^8.57.1",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "prettier": "3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  }
}
